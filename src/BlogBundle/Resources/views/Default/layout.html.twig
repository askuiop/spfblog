                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                <li><a href="#">Action</a></li>
                                <li><a href="#">Another action</a></li>
                                <li><a href="#">Something else here</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a href="#">Separated link</a></li>
                            </ul>
                        </div>
                        <div class="pull-right fast-action-notice" data-toggle="tooltip" data-placement="bottom" title="xxxx"><i class="glyphicon glyphicon-bell" aria-hidden="true"></i></div>
                        <div class="pull-right fast-action-post"><i class="glyphicon glyphicon-plus" aria-hidden="true"></i></div>

                        <div class="pull-right mh-min-search"><i class="glyphicon glyphicon-search" aria-hidden="true"></i></div>



                    </div>
                </div>



            {% endblock %}
        </div>

        <div class="clearfix"></div>

        
        <div id="page-container" class="">
            {% block page_container %}
                <div id="guide-container" class="hidden-xs col-sm-3 col-md-3">
                    {% block guide_container %}

                        <ul class="nav">
                            <li><a href="##">home</a></li>
                            <li><a href="##">post</a></li>
                            <li><a href="##">contact</a></li>
                            <li><a href="##">other</a></li>
                        </ul>

                        <div class="nav-plus">
                            <h2 class="">Something else</h2>
                            <ul class="nav">
                                <li><a href="##">some</a></li>
                                <li><a href="##">other</a></li>
                                <li><a href="##">thinks</a></li>
                            </ul>
                        </div>

                        <div class="nav-plus">
                            <div id="" class="guide-right">
                                <div id="" class="web-info" >
                                    <a href="//www.youtube.com/yt/about/zh-CN/">关于</a>
                                    <a href="//www.youtube.com/yt/copyright/zh-CN/">版权</a>
                                    <a href="//www.youtube.com/yt/creators/">创作者</a>
                                    <a href="//www.youtube.com/yt/dev/zh-CN/">开发者</a>
                                </div>
                                <div id="copyright" class="copyright">
                                    <div>© 2017 xxxx, LLC</div>
                                </div>
                            </div>
                        </div>

                    {% endblock %}
                </div>
                <div id="container" class="col-xs-12 col-sm-9 col-md-9">
